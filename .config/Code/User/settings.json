{
  "window.customMenuBarAltFocus": false,
  "workbench.startupEditor": "none",
  "files.autoSave": "afterDelay",
  "vscode-neovim.neovimInitVimPaths.linux": "/home/georgiy/.config/nvim/init.lua",
  "debug.javascript.terminalOptions": {},
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.fontFamily": "'VictorMono Nerd Font', 'JetBrainsMono Nerd Font',  'RobotoMono Nerd Font',  'Droid Sans Mono', 'monospace', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.mouseWheelZoom": true,
  "window.zoomLevel": 1,
  "editor.rulers": [80],
  "workbench.tree.renderIndentGuides": "always",
  "editor.cursorBlinking": "solid",
  "cSpell.language": "en,ru",
  "editor.codeActionsOnSave": {},
  "php.validate.executablePath": "",
  "editor.lineNumbers": "relative",
  "search.showLineNumbers": true,
  "git.autofetch": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "explorer.confirmDragAndDrop": false,

  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "isort.args": ["--profile", "black"],
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "workbench.preferredHighContrastColorTheme": "Catppuccin Mocha",
  "workbench.preferredHighContrastLightColorTheme": "Catppuccin Latte",
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "window.autoDetectColorScheme": true,
  "vscode-neovim.revealCursorScrollLine": true,
  "editor.stickyScroll.enabled": true,
  "editor.renderWhitespace": "none",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 2,
  "editor.minimap.autohide": true,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.cursorSurroundingLines": 8,
  "editor.smoothScrolling": true,
  "vscode-neovim.mouseSelectionDebounceTime": 0,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.overviewRulerBorder": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "editor.codeLensFontFamily": "'VictorMono Nerd Font', 'JetBrainsMono Nerd Font',  'RobotoMono Nerd Font',  'Droid Sans Mono', 'monospace', monospace",
  "editor.inlayHints.fontFamily": "'MonaspiceRn Nerd Font', 'GohuFont 11 Nerd Font',  'VictorMono Nerd Font', monospace",
  "terminal.integrated.fontFamily": "'VictorMono Nerd Font', 'JetBrainsMono Nerd Font',  'RobotoMono Nerd Font',  'Droid Sans Mono', 'monospace', monospace",
  "notebook.output.fontFamily": "'VictorMono Nerd Font', 'JetBrainsMono Nerd Font',  'RobotoMono Nerd Font',  'Droid Sans Mono', 'monospace', monospace",
  "errorLens.fontFamily": "'VictorMono Nerd Font', 'JetBrainsMono Nerd Font',  'RobotoMono Nerd Font',  'Droid Sans Mono', 'monospace', monospace",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "hidden",
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.list.smoothScrolling": true,
  "workbench.editor.sharedViewState": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.minimap.size": "fit",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.colorTheme": "Catppuccin Mocha",
  // we try to make semantic highlighting look good
  "editor.semanticHighlighting.enabled": true,
  // prevent VSCode from modifying the terminal colors
  "terminal.integrated.minimumContrastRatio": 1,
  // make the window's titlebar use the workbench colors
  "window.titleBarStyle": "custom",

  // applicable if you use Go, this is an opt-in flag!
  "gopls": {
    "ui.semanticTokens": true
  },
  "catppuccin.workbenchMode": "flat",
  "catppuccin.colorOverrides": {
    "mocha": {
      "base": "#181825",
      "mantle": "#11111b",
      "crust": "#11111b"
    }
  },
  "editor.inlayHints.fontSize": 12,
  "errorLens.fontStyleItalic": true,
  "catppuccin.italicKeywords": false
}
